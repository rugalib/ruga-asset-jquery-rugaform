{
  "name": "rugalib/ruga-asset-jquery-rugaform",
  "description": "Rugalib form plugin for jquery",
  "homepage": "https://github.com/rugalib",
  "type": "library",
  "require": {
    "php": "^7.4",
    "components/jquery": "*",
    "rugalib/asset-jquery-validation": "*",
    "rugalib/asset-alertifyjs": "*",
    "rugalib/asset-jquery-hotkeys": "*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.rugalib.ch/"
    }
  ],
  "authors": [
    {
      "name": "Roland Rusch",
      "email": "roland.rusch@easy-smart.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ruga\\Asset\\jquery_rugaform\\": "src/"
    }
  },
  "extra": {
    "laminas": {
      "component": "Ruga\\Asset\\jquery_rugaform",
      "config-provider": "Ruga\\Asset\\jquery_rugaform\\ConfigProvider"
    }
  }
}